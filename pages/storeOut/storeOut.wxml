<view class='big-box'>
  <!--左侧栏-->
  <view class="left-menu">
    <block wx:for="{{leftList}}" wx:key="">
      <view class='{{index == idx ? "avtive":"Default-Class"}}' bindtap="switchClassfun" data-index="{{item.index}}">{{item.title}}</view>
    </block>
  </view>
  <!--右侧栏-->
  <view class="nav_right" style='width:73%;'>
    <van-swipe-cell right-width="{{ 65 }}" left-width="{{ 65 }}">
      <view slot="left">选择</view>
      <van-cell-group>
        <van-cell title="单元格" value="内容" />
      </van-cell-group>
      <view slot="right">删除</view>
    </van-swipe-cell>
  </view>
</view>